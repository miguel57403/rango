<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/materialToolbar"
        android:layout_width="match_parent"
        android:layout_height="126dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="-8dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_marginTop="200dp"
        android:text="@string/title_profile"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#FF9800"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/materialToolbar" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="81dp"
        android:layout_height="91dp"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="373dp"
        android:layout_height="291dp"
        android:layout_marginTop="56dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/button_profile"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/white"
            android:clickable="true"
            android:contentDescription="@string/button_allergy_description"
            android:drawableLeft="@drawable/location_on_48px"
            android:drawableRight="@drawable/arrow_forward_ios_48px"
            android:foreground="?attr/selectableItemBackground"
            android:foregroundTint="@color/orange_500"
            android:text="@string/address_label"
            android:textAlignment="textStart"
            android:textColor="@color/orange_500"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="0.147"
            app:strokeColor="@color/button_allergy"
            app:strokeWidth="2dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/button_profile_one"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/white"
            android:clickable="true"
            android:contentDescription="@string/button_allergy_description"
            android:drawableLeft="@drawable/block_48px"
            android:drawableRight="@drawable/arrow_forward_ios_48px"
            android:foreground="?attr/selectableItemBackground"
            android:foregroundTint="@color/orange_500"
            android:text="@string/restrictions"
            android:textAlignment="textStart"
            android:textColor="@color/orange_500"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="0.147"
            app:strokeColor="@color/button_allergy"
            app:strokeWidth="2dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/button_profile_two"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/white"
            android:clickable="true"
            android:contentDescription="@string/button_allergy_description"
            android:drawableLeft="@drawable/order_approve_48px"
            android:drawableRight="@drawable/arrow_forward_ios_48px"
            android:foreground="?attr/selectableItemBackground"
            android:foregroundTint="@color/orange_500"
            android:text="@string/order"
            android:textAlignment="textStart"
            android:textColor="@color/orange_500"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="0.147"
            app:strokeColor="@color/button_allergy"
            app:strokeWidth="2dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/button_profile_three"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/white"
            android:clickable="true"
            android:contentDescription="@string/button_allergy_description"
            android:drawableLeft="@drawable/wallet_48px"
            android:drawableRight="@drawable/arrow_forward_ios_48px"
            android:foreground="?attr/selectableItemBackground"
            android:foregroundTint="@color/orange_500"
            android:text="@string/payment"
            android:textAlignment="textStart"
            android:textColor="@color/orange_500"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="0.147"
            app:strokeColor="@color/button_allergy"
            app:strokeWidth="2dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/button_profile_four"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/white"
            android:clickable="true"
            android:contentDescription="@string/button_allergy_description"
            android:drawableLeft="@drawable/settings_48px"
            android:drawableRight="@drawable/arrow_forward_ios_48px"
            android:foreground="?attr/selectableItemBackground"
            android:foregroundTint="@color/orange_500"
            android:text="@string/settings"
            android:textAlignment="textStart"
            android:textColor="@color/orange_500"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="0.147"
            app:strokeColor="@color/button_allergy"
            app:strokeWidth="2dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/button_profile_five"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/white"
            android:clickable="true"
            android:contentDescription="@string/button_allergy_description"
            android:drawableLeft="@drawable/info_48px"
            android:drawableRight="@drawable/arrow_forward_ios_48px"
            android:foreground="?attr/selectableItemBackground"
            android:foregroundTint="@color/orange_500"
            android:text="@string/about"
            android:textAlignment="textStart"
            android:textColor="@color/orange_500"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="0.147"
            app:strokeColor="@color/button_allergy"
            app:strokeWidth="2dp"
            tools:ignore="MissingConstraints,RtlHardcoded" />
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/materialToolbar"
        tools:ignore="MissingConstraints">

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imvCircularWithStroke"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginEnd="128dp"
            android:padding="5dp"
            android:src="@drawable/photo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/imvCircular"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:shapeAppearanceOverlay="@style/Circular"
            app:strokeColor="@color/white"
            app:strokeWidth="4dp"
            tools:ignore="UnknownId" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>